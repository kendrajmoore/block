{
  "name": "block",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "jest --watchAll",
    "start": "npm run build-client && node server.js",
    "dev": "npm run dev-client & npm run start-redis && nodemon server.js",
    "dev-peer": "cross-env GENERATE_PEER_PORT='true' nodemon server.js",
    "start-redis": "redis-server --daemonize yes",
    "build-client": "npm run clean && parcel build client/src/index.html --out-dir client/dist",
    "dev-client": "npm run clean && parcel client/src/index.html --out-dir client/dist",
    "clean": "rm -rf .cache client/dist"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "jest": "^23.6.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "elliptic": "^6.4.1",
    "express": "^4.16.4",
    "hex-to-binary": "^1.0.1",
    "parcel-bundler": "^1.10.3",
    "pubnub": "^4.21.6",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "redis": "^2.8.0",
    "uuid": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kendrajmoore/block.git"
  },
  "bugs": {
    "url": "https://github.com/kendrajmoore/block/issues"
  },
  "homepage": "https://github.com/kendrajmoore/block#readme",
  "description": ""
}
